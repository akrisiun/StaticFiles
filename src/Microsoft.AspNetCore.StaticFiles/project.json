{
  "version": "1.1.0-beta5",
  "buildOptions": {
    "warningsAsErrors": false,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": false
  },
  "description": "ASP.NET Core static files middleware. Includes middleware for serving static files, directory browsing, and default files.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/staticfiles"
    },
    "tags": [
      "aspnetcore",
      "staticfiles"
    ]
  },
  "dependencies": {
    "Microsoft.AspNetCore.Http.Extensions": "1.1.0-*",
    "Microsoft.AspNetCore.Hosting.Abstractions": "1.1.0-*",

    "Microsoft.AspNetCore.Http.Features": "1.1.0-*",
    "Microsoft.AspNetCore.Http.Abstractions": "1.1.0-*",
    "Microsoft.AspNetCore.WebUtilities": "1.1.0-*",
    
    "Microsoft.Extensions.FileProviders.Abstractions": "1.1.0-*",
    "Microsoft.Extensions.Logging.Abstractions": "1.1.0-*",
    "Microsoft.Extensions.WebEncoders": "1.1.0-*"
  },
  "frameworks": {
    "net451": {
        "dependencies": {
        },
        "frameworkAssemblies": {
